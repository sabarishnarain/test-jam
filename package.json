{
  "name": "test-jam",
  "version": "1.0.0",
  "description": "Joint Dashboard for Automation and Manual testing",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter html cross-env TESTJAM_ENV=test mocha --require ts-node/register ./test/unit/*.ts",
    "test-ci": "nyc cross-env TESTJAM_ENV=test mocha --reporter mocha-junit-reporter --require ts-node/register ./test/unit/*.ts",
    "test-integ" : "wdio wdio.conf.js",
    "dev": "tsc && gulp && cross-env TESTJAM_ENV=dev node dist/server.js",
    "prod": "tsc && gulp && cross-env TESTJAM_ENV=prod node dist/server.js",
    "demo": "tsc && gulp && cross-env TESTJAM_ENV=demo node dist/server.js",
    "lint": "tsc && tslint -p ."
  },
  "author": "snarain",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/chai-string": "^1.4.2",
    "@types/mocha": "^5.2.7",
    "@wdio/cli": "^5.14.4",
    "@wdio/junit-reporter": "^5.14.4",
    "@wdio/local-runner": "^5.14.4",
    "@wdio/mocha-framework": "^5.14.4",
    "@wdio/selenium-standalone-service": "^5.13.2",
    "@wdio/spec-reporter": "^5.14.4",
    "@wdio/sync": "^5.14.4",
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "cross-env": "^6.0.2",
    "mocha": "^6.2.0",
    "mocha-junit-reporter": "^1.23.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/ejs": "^2.6.3",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/fs-extra": "^8.0.0",
    "@types/node": "^12.7.5",
    "@types/node-sass-middleware": "0.0.31",
    "@types/uuid": "^3.4.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "fs": "0.0.1-security",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "node-sass-middleware": "^0.11.0",
    "node-ts": "^5.0.2",
    "nyc": "^14.1.1",
    "typescript": "^3.6.3",
    "uuid": "^3.3.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/coverage",
      "dist/client/**",
      "src/client/**",
      "src/renderers/**",
      "src/routes/**",
      "src/server.ts",
      "test/**",
      "gulpfile.js"
    ],
    "reporter": [
      "cobertura"
    ],
    "all": true
  }
}
